programa {
  funcao inicio() {
    
    inteiro num,num2
    escreva("escreve um numero inteiro \n")
    leia(num)
    escreva("escreve outro numero inteiro \n")
    leia(num2)
    escreva("\n adição: ",(num+num2))
    escreva("\n subtração: ",(num-num2))
    escreva("\n divisão: ",(num/num2))
    escreva("\n multiplicação: ",(num*num2))
  }
}
